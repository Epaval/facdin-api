{
  "name": "facdin-api",
  "version": "1.0.0",
  "description": "API REST para Facturación Digital en Venezuela",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:check": "node src/scripts/check-postgres.js",
    "db:sync": "node scripts/sync-db.js",
    "db:reset": "node scripts/sync-db.js --force",
    "db:seed": "node scripts/seed.js",
    "db:setup": "npm run db:sync && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs:generate": "swagger-jsdoc -d src/docs/swagger.json -o src/docs/swagger.json"
  },
  "keywords": [
    "facturación",
    "venezuela",
    "api",
    "postgresql",
    "seniat"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "xml2js": "^0.6.2",
    "pdfkit": "^0.13.0",
    "multer": "^1.4.5",
    "joi": "^17.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "sequelize-cli": "^6.6.2"
  }
}